{
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "name": "Hakgok Eco",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "bookmarks",
    "tabs",
    "scripting",
    "activeTab",
    "webNavigation"
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  }
}
